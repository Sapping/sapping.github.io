//meta(http-equiv="refresh", content="1; url=/404.html")
h1.center Journal Posts
    p
    ul
        each article, slug in public.journal._data
            li
                a(href="/journal/#{ slug }")
                    h3 #{ article.title }
                h4 Published #{ article.date }
                    if article.firstpostdate
                        |, originally posted on #{ article.firstpostdate}

